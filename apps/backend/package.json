{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "registry:scan": "tsx src/scripts/scan-registry.ts",
    "registry:scan:openstax": "tsx src/scripts/scan-registry.ts --seed=OpenStax",
    "registry:scan:python": "tsx src/scripts/scan-registry.ts --seed=\"Python Documentation\"",
    "registry:list-seeds": "tsx src/scripts/scan-registry.ts --list-seeds",
    "registry:list-sources": "tsx src/scripts/scan-registry.ts --list-sources",
    "registry:list-assets": "tsx src/scripts/scan-registry.ts --list-assets",
    "registry:export": "tsx src/scripts/scan-registry.ts --export-only",
    "registry:scan:export": "tsx src/scripts/scan-registry.ts --export",
    "registry:test": "tsx src/scripts/test-registry.ts",
    "registry:test:full": "tsx src/scripts/test-registry.ts --full"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.86.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "openai": "^4.76.1",
    "resend": "^6.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
