SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
GEMINI_API_KEY=your_google_ai_key
GEMINI_MODEL=gemini-2.0-flash-exp
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-5

# Ollama Configuration (Development Only)
# Set USE_OLLAMA=true to use local Ollama models instead of OpenAI
# This will only work when NODE_ENV is not set to 'production'
USE_OLLAMA=false
OLLAMA_BASE_URL=http://localhost:11434/v1
OLLAMA_MODEL=llama3.2

# Node environment (development/production)
NODE_ENV=development

RESEND_API_KEY=re_xxxxxxxxx
# Get your API key from https://resend.com/api-keys

WAITLIST_FROM_EMAIL=noreply@yourdomain.com
# Must be a verified domain in your Resend dashboard
# For development, you can use: onboarding@resend.dev

FIRECRAWL_BASE_URL=http://localhost:3002
FIRECRAWL_AGENT_BASE_URL=https://api.firecrawl.dev/v2
FIRECRAWL_API_KEY=
USE_FIRECRAWL=true
FIRECRAWL_CRAWL_URLS=
FIRECRAWL_EXTRACT_URLS=
FIRECRAWL_AGENT_URLS=
FIRECRAWL_AGENT_MODEL=spark-1-mini
FIRECRAWL_AGENT_MAX_CREDITS=
FIRECRAWL_USE_LOCAL_SEARCH=true
FIRECRAWL_SEARCH_LIMIT=12

# Source Registry (set true to enable registry endpoints)
ENABLE_SOURCE_REGISTRY=false